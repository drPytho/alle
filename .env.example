# PostgreSQL connection string
POSTGRES_URL=postgresql://localhost/postgres

# WebSocket server bind address
WS_BIND_ADDR=127.0.0.1:8080

# Comma-separated list of channels to listen on initially (optional)
# Leave empty to use only dynamic subscriptions
LISTEN_CHANNELS=notifications,events,alerts

# Log level (optional)
RUST_LOG=alle=info,info
